            "parent_id": "4377e227-5a23-4903-9360-49464d9598d5",
            "parent_table": "block",
            "properties": {
              "language": [
                [
                  "Plain Text"
                ]
              ],
              "title": [
                [
                  "#pragma warning(push)\n#pragma warning(disable : 4668) // silence /analyze: de-referencing a nullptr pointer\n#endif\n#include \u003cwindows.h\u003e\n... and other system headers\n#pragma warning(pop)"
                ]
              ]
            },
            "type": "code",
            "version": 4,
            "code": "#pragma warning(push)\n#pragma warning(disable : 4668) // silence /analyze: de-referencing a nullptr pointer\n#endif\n#include \u003cwindows.h\u003e\n... and other system headers\n#pragma warning(pop)",
            "code_language": "Plain Text"
          },
