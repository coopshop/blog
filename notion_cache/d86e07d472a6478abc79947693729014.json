            "parent_table": "block",
            "properties": {
              "title": [
                [
                  "There are 2 solutions."
                ]
              ]
            },
            "type": "text",
            "version": 4,
            "inline_content": [
              {
                "Text": "There are 2 solutions."
              }
            ]
          },
          {
            "alive": true,
