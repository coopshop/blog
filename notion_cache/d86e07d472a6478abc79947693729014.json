            "parent_table": "block",
            "properties": {
              "title": [
                [
                  "You can enable this warning globally and disable when including system headers:"
                ]
              ]
            },
            "type": "text",
            "version": 4,
            "inline_content": [
              {
                "Text": "You can enable this warning globally and disable when including system headers:"
              }
            ]
          },
          {
            "alive": true,
            "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
            "created_time": 1531204194228,
            "id": "85edd391-876a-445f-aec3-f9972089136c",
