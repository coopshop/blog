        "parent_table": "block",
        "properties": {
          "title": [
            [
              "This is useful e.g. when you have a repository with common code useful in multiple projects."
            ]
          ]
        },
        "type": "text",
        "version": 138,
        "inline_content": [
          {
            "Text": "This is useful e.g. when you have a repository with common code useful in multiple projects."
          }
        ]
      },
