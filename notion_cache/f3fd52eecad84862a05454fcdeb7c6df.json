        "properties": {
          "title": [
            [
              "Subversion has a way of including the content of one repository in another repository."
            ]
          ]
        },
        "type": "text",
        "version": 19,
        "inline_content": [
          {
            "Text": "Subversion has a way of including the content of one repository in another repository."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
