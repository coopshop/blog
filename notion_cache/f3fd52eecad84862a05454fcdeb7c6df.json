        "properties": {
          "title": [
            [
              "Using property  "
            ],
            [
              "svn:externals",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " you can tell subversion to fetch content of external repository into a given directory."
            ]
          ]
        },
        "type": "text",
        "version": 89,
        "inline_content": [
          {
            "Text": "Using property  "
          },
          {
            "Text": "svn:externals",
            "AttrFlags": 2
          },
          {
            "Text": " you can tell subversion to fetch content of external repository into a given directory."
          }
        ]
