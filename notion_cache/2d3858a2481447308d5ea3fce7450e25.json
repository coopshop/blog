          "title": [
            [
              "/* Return TRUE if file 'fileName' exists */\nbool FileExists(const TCHAR *fileName)\n{\n    DWORD       fileAttr;\n\n    fileAttr = GetFileAttributes(fileName);\n    if (0xFFFFFFFF == fileAttr)\n        return false;\n    return true;\n}"
            ]
          ]
